<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OrganizerActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="This is the organizer activity." />

        <Button
            android:id="@+id/eventManagementBTN"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onEventManagementClick"
            android:text="@string/eventManagementButton" />

        <Button
            android:id="@+id/eTypeAssociationBTN"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onAssociateTypesClick"
            android:text="Associate Club Profie With Event Types" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>